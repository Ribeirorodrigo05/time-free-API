{
  "plugins": ["unused-imports"],
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  // "env": {
  //   "browser": true,
  //   "es6": true,
  //   "node": true,
  //   "jest": true,
  //   "commonjs": true,
  //   "es2020": true
  // },
  "rules": {
    "no-unused-vars": "off",
    "unused-imports/no-unused-imports": "error",
    "no-console": "true",
    "unused-imports/no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "varsIgnorePattern": "^_",
        "args": "after-used",
        "argsIgnorePattern": "^_"
      }
    ]
  }
}
